# PostgreSQL connection string (required format)
DATABASE_URL="postgresql://username:password@localhost:5432/apexsalesai?schema=public"

# Other environment variables
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret"

# API Keys
RESEND_API_KEY="your-resend-api-key"
HUBSPOT_CLIENT_ID="your-hubspot-client-id"
HUBSPOT_CLIENT_SECRET="your-hubspot-client-secret"
HUBSPOT_REDIRECT_URI="http://localhost:3000/api/auth/callback/hubspot"

# Feature flags
ENABLE_AGENT_AUTONOMY="true"
ENABLE_MOCK_DATA_FALLBACK="true"
